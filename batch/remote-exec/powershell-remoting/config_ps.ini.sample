[Server]
REMOTE_SERVER=192.168.1.100
REMOTE_USER=Administrator
REMOTE_BATCH_PATH=C:\Scripts\target_script.bat

[Options]
# バッチファイルに渡す引数（オプション）
BATCH_ARGUMENTS=

# 実行結果を保存するログファイル
OUTPUT_LOG=remote_exec_output.log

# HTTPS使用（1=使用する, 0=使用しない）
USE_SSL=0

; 注意: セキュリティ上、パスワードはここに記載せず、
;       実行時に入力することを推奨します。
