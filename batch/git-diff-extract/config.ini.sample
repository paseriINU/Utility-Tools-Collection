[Branches]
# 比較元ブランチ（基準となるブランチ）
BASE_BRANCH=main

# 比較先ブランチ（差分を取得したいブランチ）
TARGET_BRANCH=develop

[Output]
# 出力先フォルダ名（リポジトリルートからの相対パスまたは絶対パス）
OUTPUT_DIR=diff_output

# 削除されたファイルも含めるか（1=含める, 0=含めない）
# 0の場合、追加・変更されたファイルのみ抽出
INCLUDE_DELETED=0

# 追加設定例：
# OUTPUT_DIR=C:\Temp\MyProject_Diff
# OUTPUT_DIR=..\deploy\差分ファイル
